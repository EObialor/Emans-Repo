import re

pattern = r"^(?=.*[A-Za-z])(?=.*\d)(.*[A-Z])(?=.*[a-z])(?=.*[@#$%*!]).{8,}$"

def user_pass():
    user_pass = input("Enter your password: ")
password = "Passw0rd!"

if user_pass != re.match(pattern, password):
    print("That's a good password")
else:
    print("That's not a good password")

